public class Bidimensional2 {

	public static void main(String[] args) {
		
		int[][] a4 = new int[50][100];
    	int i = 0;
    	int j = 0;
    	
    	for(i = 0; i < 50; i++)
    		for(j = 0; j < 100; j++)
    			a4[i][j] = (50 - i) + (100 - j);
    	
    	System.out.println("\tElemento Inverso (Array): \n");
    	
    	for(i = 0; i < 50; i++)
    		for(j = 0; j < 100; j++) 
    			System.out.println("Elemento ["+ i +"]["+ j +"]: "+ a4[i][j]);
    	
	}
}
