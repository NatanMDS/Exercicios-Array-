public class Inverso {

	public static void main(String[] args) {
		
		int[] a2 = new int[9];
    	int i = 0;
    	
    	for (i = 0; i < 8; i++)
    			a2[i] = 8 - i;
    	
    	System.out.println("\tElemento Inverso (Array): \n");
    	
    	for(i = 0; i < 9; i++) 
            System.out.println("Elemento ["+ i +"]: "+ a2[i]);
    	
	}
}
